{
  "name": "mineflayer-internal-plugins",
  "version": "1.0.0",
  "description": "mineflayer internal plugins",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "types": "./dist/typings/index.d.ts",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "echo 'Build started'&&npx tsc&& npx tsc -p tsconfig.esm.json &&echo 'Build finished'",
    "start": "clear&&cd src &&ts-node ./index.ts",
    "dev": "nodemon --watch ./src -e ts --exec 'npm run start' "
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeneSatka/mineflayer-internal-plugins.git"
  },
  "keywords": [
    "mineflayer",
    "minecraft",
    "myncraftjs",
    "mcbot",
    "minecraftbot",
    "mineflayerjs",
    "senesatka"
  ],
  "author": {
    "email": "senesatka@gmail.com",
    "name": "SeneSatka"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeneSatka/mineflayer-internal-plugins.git/issues"
  },
  "homepage": "https://github.com/SeneSatka/mineflayer-internal-plugins.git#readme",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "minecraft-data": "^3.66.0",
    "minecraft-protocol": "^1.47.0",
    "prismarine-biome": "^1.3.0",
    "prismarine-block": "^1.17.1",
